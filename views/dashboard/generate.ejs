                        </li>
                        <li>
                            <a href="/sds/ui"> <i class="menu-icon fa fa-dashboard"></i>Voucher Management </a>
                        </li>
                    <% }else{ %>
                        <% if(role == 'admin') { %> 
                            <li>
                                <a href="/sds/upload/records"> <i class="menu-icon fa fa-dashboard"></i>Upload Records </a>
                            </li>
                        <% }else{ %>
