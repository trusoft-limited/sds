                              <li><i class="fa fa-bars"></i><a href="/sds/changepassword">Change Password</a></li>
                          </ul>
                    </li>

                    <h3 class="menu-title">CLOSE</h3><!-- /.menu-title -->
                    <li>
                        <a id="logout_cool" href="/sds/dashboard"> <i class="menu-icon fa fa-dashboard"></i>Logout </a>
                    </li>

                </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
    </aside><!-- /#left-panel -->

    <!-- Left Panel -->

    <!-- Right Panel -->

    <div id="right-panel" class="right-panel">

        <!-- Header-->
        <header id="header" class="header">

            <div class="header-menu">

                <div class="col-sm-7">
                    <a id="menuToggle" class="menutoggle pull-left"><i class="fa fa fa-tasks"></i></a>
                    <div class="header-left">
                        
                        
                    </div>
                </div>
            </div>

        </header><!-- /header -->

      <div class="breadcrumbs">
        <div class="col-sm-4">
            <div class="page-header float-left">
                <div class="page-title">
                    <h1 id='uuname'><%= name %></h1>
                </div>
            </div>
        </div>
        <div class="col-sm-8">
            <div class="page-header float-right">
                <div class="page-title">
                    <ol class="breadcrumb text-right">
                        <li id="time_update" class="active"><%= time %></li>
                    </ol>
                </div>
            </div>
        </div>
      </div>

        <div class="content mt-3">
            <div class="animated fadeIn">
                <div class="row">

                <div id="display" class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title">Vouchers</strong>
                        </div>
                        <div class="card-body">
                  <table id="bootstrap-data-table" class="table table-striped table-bordered">
                    <thead>
                      <tr>
                        <th>Batch Number</th>
                        <th>Amount</th>
                        <th>Quantity</th>
                        <th>Download</th>
                      </tr>
                    </thead>
                    <tbody>
                        <% if(ej) { %> 
                            <% for(var i = (ej.length - 1); i >= 0; i-- ) { %>
                                <tr>
                                    <td><%= ej[i].batchnumber %></td>
                                    <td id="at<%= ej[i].id %>"></td>
                                    <script>
                                        var amt = '<%= ej[i].amount %>';
                                        var id = 'at<%= ej[i].id %>';
                                        var t = parseFloat(amt);
                                        var famt = (t).toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$&,');
                                        document.getElementById(id).innerHTML = "NGN " + famt;
                                    </script>
                                    <td><%= ej[i].quantity %></td>
                                    <td><a href="<%= ej[i].file %>" target="_blank"><i class="fa fa-download"></i> Download Batch</a></td>
                                </tr>
                            <% } %>
                        <% } %>
                    </tbody>
                  </table>
                        </div>

                        <button style="float: center;" onclick="newHost()" type="button" class="btn btn-outline-primary">Generate Voucher</button>
                    


                        


                    
                    </div>


                    
                </div>
                
                <div id="editnew1" style="display:none" class="col-lg-6">
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title">New Vouchers</strong>
                        </div>
                        <div class="card-body">
                          <!-- Credit Card -->
                          <div id="pay-invoice">
                                <div>
                                        <button id="newLabelClick" type="button" class="btn btn-lg btn-info btn-block">
                                            <span id="payment-button-amount">Add Voucher</span>
                                        </button>
                                    </div>

                              <div id="labBtn" class="card-body">
                                  <form id="fdet" action="" method="post" >
                                      
                                      

                                    <!--<div class="form-group">
                                        <label for="cc-payment" class="control-label mb-1">Convenience Fee</label>
                                        <input id="conveniencefee" name="cc-payment" type="text" class="form-control" aria-required="true" aria-invalid="false" maxlength="50" value="">
                                    </div>
                                      
                                    <div class="form-group">
                                        <label for="cc-ssl" class="control-label mb-1">Print Validation</label>
                                        <select style="color: #001e33; font-weight: bold;" name="cc-ssl" id="printvalidation" class="form-control">
                                            <option style="color: #001e33; font-weight: bold;" value="true">True</option>
                                            <option style="color: #001e33; font-weight: bold;" value="false">False</option>
                                        </select>
                                    </div>-->
                                    

                                    


                              </div>
                              
                          </div>

                        </div>
                    </div> <!-- .card -->
                    <hr><hr>
                    <div>
                      <button id="payment-button" style="float: center;" onclick="backHost()" type="button" class="btn btn-lg btn-info btn-block">Generate</button>
                    </div>
                    
                  </div><!--/.col-->

                </form>

                </div><!--/.col-->



                
                </div>

                
            </div><!-- .animated -->
        </div><!-- .content -->


